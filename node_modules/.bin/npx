#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/npx@10.2.2/node_modules/npx/node_modules:/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/npx@10.2.2/node_modules:/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/npx@10.2.2/node_modules/npx/node_modules:/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/npx@10.2.2/node_modules:/home/krajesh/coffeeucd.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npx/index.js" "$@"
else
  exec node  "$basedir/../npx/index.js" "$@"
fi
